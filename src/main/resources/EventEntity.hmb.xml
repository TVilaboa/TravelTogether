<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>


    <class name="hibernate.EventsEntity" table="EVENTS" schema="PUBLIC" catalog="PUBLIC">
        <id name="id">
            <column name="ID" sql-type="integer" not-null="true"/>
        </id>
        <property name="title">
            <column name="TITLE" sql-type="varchar" length="100"/>
        </property>
        <property name="url">
            <column name="URL" sql-type="varchar" length="100"/>
        </property>
        <property name="clazz">
            <column name="CLASS" sql-type="varchar" length="100"/>
        </property>
        <property name="start">
            <column name="START" sql-type="integer"/>
        </property>
        <property name="end">
            <column name="END" sql-type="integer"/>
        </property>
    </class>
</hibernate-mapping>